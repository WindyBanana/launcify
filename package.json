{
  "name": "launcify-template",
  "version": "2.0.0",
  "description": "AI-first Next.js template for non-developers. Build production apps by talking to your AI assistant.",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "setup-context7": "node .ai/scripts/setup-context7.js",
    "cleanup-unused": "node .ai/scripts/cleanup-configs.js"
  },
  "_comment": "NOTE: Users should NEVER run these scripts directly. The AI assistant runs all commands for them.",
  "dependencies": {
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/ui": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.0.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/launcify-template"
  },
  "keywords": [
    "nextjs",
    "template",
    "ai-first",
    "cursor",
    "windsurf",
    "claude-code",
    "non-developers",
    "no-code"
  ],
  "license": "MIT"
}
